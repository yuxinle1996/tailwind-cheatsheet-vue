{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "baseUrl": "./",
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    // "types": [
    //   "element-plus/global"
    // ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "*": [
        // "node_modules/*",
        "src/types/*"
      ]
    },
  },
  "include": [
    "src/**/*.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ]
}